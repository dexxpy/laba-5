#include <iostream>

using namespace std;


int main()
{
	setlocale(0, "");

	double a = 3;
	double b = -4;
	double c = 9;

	double dis = b * b - 4 * a * c;

	if (dis > 0)
	{
		double x1 = (-b + sqrt(dis)) / (2 * a);
		double x2 = (-b - sqrt(dis)) / (2 * a);
		cout << "x1 = " << x1 << endl;
		cout << "x2 = " << x2 << endl;
	}
	else if (dis == 0)
	{
		double x1 = (-b / (2 * a));

		cout << "x1 = " << x1 << endl;
	}
	else {
		cout << "Нет корней" << endl;
	}


}
